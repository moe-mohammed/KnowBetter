@{
    ViewData["Title"] = "KnowBetter Dashboard";
    Layout = "~/Views/Shared/Site_Layout.cshtml";
}

<h1 align="center" style="margin-top:10px">Welcome, @ViewBag.MyName</h1>
<hr />
<h3><a href="@Url.Action("CompareProductSelect","Products")" class="btn btn-primary" style="background-color: rgba(121, 161, 173, 1);  border-color: rgb(65 113 127);">Product Compare</a></h3>
<h3><a href="@Url.Action("IngredientLibrary","Ingredients")" class="btn btn-primary" style="background-color: rgba(121, 161, 173, 1); border-color: rgb(65 113 127); ">Ingredient Library</a></h3>
<h3><a href="@Url.Action("EditProfile","Users")"class="btn btn-primary" style="background-color: rgba(121, 161, 173, 1); border-color: rgb(65 113 127); ">Edit Profile</a></h3>
<h3><a href="@Url.Action("Logout", "Users")"class="btn btn-primary" style="background-color: rgba(220, 186, 159, 1); border-color: rgb(65 113 127); ">Logout</a></h3>